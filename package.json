{
  "name": "sb-addon-permutation-table",
  "version": "2.0.0",
  "description": "Storybook addon presenting component with table",
  "keywords": [
    "storybook-addons",
    "storybook-addons",
    "style",
    "appearance"
  ],
  "repository": "https://github.com/daimresearch/sb-addon-permutation-table.git",
  "type": "module",
  "author": "daim-FE",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "start": "npm run build && npm run storybook",
    "release": "yarn build && npm publish",
    "storybook": "storybook dev -p 6006",
    "vite": "vite build --watch --config vite.watch.config.ts",
    "storybook:build": "storybook build -o ./docs && touch ./docs/.nojekyll",
    "dev": "yarn build:watch & yalc push"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-mdx-gfm": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/theming": "^8.6.4",
    "@storybook/types": "^8.6.4",
    "@types/node": "^22.5.0",
    "@types/prismjs": "^1.26.4",
    "@types/ramda": "^0.30.2",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/tinycolor2": "^1.4.6",
    "@vitejs/plugin-react": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "peerDependencies": {
    "storybook": "^8.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.tsx"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Permutation",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://github.com/daimresearch/sb-addon-permutation/assets/107913240/15b3e7a3-26a1-4262-8f62-a0728d73a23c"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@floating-ui/react": "^0.26.23",
    "prism-react-renderer": "^2.3.1",
    "ramda": "^0.30.1"
  }
}
